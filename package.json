{
  "name": "office-addin-taskpane-react",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start:backend": "nodemon --exec ts-node ./server/server.ts",
    "dev": "concurrently \"npm run dev-server\" \"npm run start:backend\"",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/msal-browser": "^4.22.0",
    "@azure/msal-react": "^3.0.19",
    "@azure/openai": "^2.0.0",
    "@fluentui/font-icons-mdl2": "^8.5.63",
    "@fluentui/react": "^8.123.6",
    "@fluentui/react-hooks": "^8.8.19",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/office-js": "^1.1.110",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "core-js": "^3.36.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "es6-promise": "^4.2.8",
    "express": "^5.1.0",
    "openai": "^5.22.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "recharts": "2.12.7",
    "regenerator-runtime": "^0.14.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.23.3",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@types/node": "^24.5.2",
    "@types/office-js": "^1.0.533",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.2.64",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^18.2.21",
    "@types/webpack": "^5.28.5",
    "@types/xlsx": "^0.0.35",
    "acorn": "^8.11.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint-plugin-office-addins": "^4.0.3",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "nodemon": "^3.1.10",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ]
}
